<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Warna default saat tombol tidak dalam keadaan ditekan atau difokuskan -->
<item android:state_pressed="false" android:state_focused="false">
    <shape android:shape="rectangle">
        <!-- Warna latar belakang default -->
        <stroke android:color="#ECE3CE" android:width="3dp"/>
        <!-- Sudut radius -->
        <corners android:radius="15dp"/>
    </shape>
</item>
<!-- Warna saat tombol dalam keadaan ditekan -->
<item android:state_pressed="true">
    <shape android:shape="rectangle">
        <!-- Warna latar belakang saat ditekan -->
        <solid android:color="#F3EEEA" />

        <!-- Sudut radius -->
        <corners android:radius="15dp" />
    </shape>
</item>
</selector>



